{
  "trailingComma": "es5",
  "endOfLine": "lf",
  "bracketSpacing": true,
  "tabWidth": 2,
  "semi": false,
  "singleQuote": false,
  "arrowParens": "always",
  "tailwindStylesheet": "./src/app/globals.css",
  "importOrder": [
    "^(react/(.*)$)|^(react$)",
    "^(next/(.*)$)|^(next$)",
    "<THIRD_PARTY_MODULES>",
    "",
    "^types$",
    "^@/(.*)$",
    "",
    "^[./]"
  ],
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ]
}
